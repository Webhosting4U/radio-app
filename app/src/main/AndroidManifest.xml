<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gxm.webapp" >
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".facebookActivity"/>
        <activity android:name=".aboutActivity"/>
        <activity android:name=".afierosisActivity"/>
        <activity android:name=".blogActivity"/>
        <activity android:name=".contactActivity"/>
        <activity android:name=".liveActivity"/>
        <activity android:name=".twitterActivity"/>
        <activity android:name=".wwwActivity"/>
        <activity android:name=".recentTracksActivity"/>
        <service
            android:enabled="true"

            android:name="myPlayService" />
    </application>

</manifest>
